<!-- registration image on the left  -->
<div id="register-container">
    <section class="register-left-side">
    </section>
<!-- registration form -->
    <section class="register-right-side">
        <form name="register" id="register" method="post" action="/register">
            <fieldset>
                <legend class="register-head">Create an account</legend>
                <p class="register-content">Takes less than a minute</p>

                <span class="register-input register-input-hoshi">
                    <input type="text" id="username" name="username" size="20" class="register-input-field register-input-field-hoshi">
                    <label for="username" class="register-input-label register-input-label-hoshi register-input-label-hoshi-color">
                        <span class="register-input-label-content register-input-label-content-hoshi">Username:</span>
                    </label>
                </span>

                <span class="register-input register-input-hoshi">
                    <input type="email" id="email" name="email" size="20" class="register-input-field register-input-field-hoshi">
                    <label for="email" class="register-input-label register-input-label-hoshi register-input-label-hoshi-color">
                        <span class="register-input-label-content register-input-label-content-hoshi">Email:</span>
                    </label>
                </span>

                <span class="register-input register-input-hoshi">
                    <input type="password" id="password" name="password" size="20" class="register-input-field register-input-field-hoshi">
                    <label for="phone" class="register-input-label register-input-label-hoshi register-input-label-hoshi-color">
                        <span class="register-input-label-content register-input-label-content-hoshi">Password:</span>
                    </label>
                </span>

                <div class="register-sign-up">
                    <button class="register-sign-up-btn float-left" name="submitted" type="submit">Sign Up</button>
                    <span><a href="/login">I am already a member</a></span>
                </div>
            </fieldset>
        </form>
    </section>
</div>    
